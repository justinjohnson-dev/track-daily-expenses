generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model expenses {
  id              String @id @default(auto()) @map("_id") @db.ObjectId
  expense         String
  expenseAmount   Float
  expenseCategory String
  expenseDate     String
  expenseMonth    Int
  userId          String
}

model income {
  id             String @id @default(auto()) @map("_id") @db.ObjectId
  incomeAmount   Float
  incomeCategory String
  incomeDate     String
  incomeName     String
  userId         String
}

model users {
  id       String @id @default(auto()) @map("_id") @db.ObjectId
  email    String
  password String
  username String
}
